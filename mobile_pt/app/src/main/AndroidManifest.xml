<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobile_pt">

    <application
        android:name=".planner.MyPlanner"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".messages.ChatLogActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.LatestMessagesActivity" />
        </activity>
        <activity android:name=".messages.NewMessageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.LatestMessagesActivity" />
        </activity>
        <activity android:name=".messages.LatestMessagesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Traineralram" />
        <activity android:name=".TrainerListActivity" />
        <activity android:name=".signinactivity" />
        <activity android:name=".DietListAdapter" />
        <activity android:name=".registerlogin.RegisterActivity" />
        <activity android:name=".registerlogin.LoginActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".FoodActivity" />
        <activity android:name=".planner.EditActivity" />
        <activity
            android:name=".planner.PlannerActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>